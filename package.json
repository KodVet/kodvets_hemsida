		"@sveltejs/kit": "^2.39.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"super-sitemap": "^1.0.5",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"vite": "^7.1.5"
